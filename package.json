{
  "name": "google-cloud-mcp",
  "version": "0.5.0",
  "description": "Model Context Protocol server for Google Cloud services",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && pnpm run copy-assets",
    "copy-assets": "cp -R src/services/monitoring/*.md dist/services/monitoring/",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "ci": "pnpm run lint && pnpm run format:check && pnpm run test:coverage"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-cloud",
    "error-reporting",
    "iam",
    "logging",
    "monitoring",
    "profiler",
    "spanner",
    "trace"
  ],
  "author": "Kristof Kowalski <k@ko.wal.ski>",
  "license": "Apache",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.4",
    "@google-cloud/iam": "^2.3.1",
    "@google-cloud/logging": "^11.2.1",
    "@google-cloud/monitoring": "^5.3.1",
    "@google-cloud/resource-manager": "^6.2.1",
    "@google-cloud/spanner": "^8.2.2",
    "@google-cloud/storage": "^7.17.3",
    "@google-cloud/trace-agent": "^8.0.0",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "winston": "^3.18.3",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=24.11.0"
  },
  "packageManager": "pnpm@10.21.0"
}
